  <template name="name_get" value="def name_get(self, cursor, uid, ids, context=None):&#10;    if not context:&#10;        context = {}&#10;    if not ids:&#10;        return []&#10;    res = []&#10;    for record in self.browse(cursor, uid, ids, context=context):&#10;        res += [(record.id, record.name)]&#10;    return res&#10;    " toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="name_search" value="def name_search(self, cursor, uid, name, args=None, operator='ilike',&#10;                context=None, limit=80):&#10;    if args is None:&#10;        args = []&#10;    if not context:&#10;        context = {}&#10;    search_params = [] + args&#10;    if name:&#10;        search_params += [&#10;            ('name', operator, name)&#10;        ]&#10;    ids = self.search(cursor, uid, search_params, limit=limit,&#10;                      context=context)&#10;    return self.name_get(cursor, uid, ids, context=context)&#10;    " toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
